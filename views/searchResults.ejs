

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Change Profile Image</title>
  <link rel="stylesheet" href="/stylesheets/index.css">
</head>
<body>
 <!-- searchResults.ejs -->
<h2>Search Results</h2>
<% if (users.length > 0) { %>
  <ul>
    <% users.forEach(user => { %>
      <li><a href="/userprofile/<%= user.username %>"><%= user.username %></a></li>
    <% }); %>
  </ul>
<% } else { %>
  <p>No users found.</p>
<% } %>

</body>
</html>
